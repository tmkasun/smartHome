$(function() {
  $(".sub_name").tokenInput("/subjects.json", {
    crossDomain: false,
    prePopulate: $(".sub_name").data("pre"),
	preventDuplicates: true,
	tokenLimit: 1,
    //theme: "mac"
  });
});
 
$(function() {
  $(".user_name").tokenInput("/users.json", {
    crossDomain: false,
    prePopulate: $(".user_name").data("pre"),
	preventDuplicates: true,
	tokenLimit: 1,
    //theme: "mac"
  });
});

